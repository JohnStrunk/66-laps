{
  "name": "66-laps",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev-notp": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . && tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "yarn coverage:clean && start-server-and-test 'yarn dev -p 3000' http://localhost:3000 'BASE_URL=http://localhost:3000 NODE_OPTIONS=\"--import tsx\" cucumber-js --parallel 2'",
    "test:static": "yarn coverage:clean && yarn build && start-server-and-test 'npx serve out -p 3000' http://localhost:3000 'BASE_URL=http://localhost:3000 NODE_OPTIONS=\"--import tsx\" cucumber-js --parallel 2'",
    "test:unit": "yarn coverage:clean && NODE_OPTIONS=\"--import tsx\" cucumber-js --tags \"not @browser\"",
    "test:e2e": "yarn coverage:clean && start-server-and-test 'yarn dev -p 3000' http://localhost:3000 'BASE_URL=http://localhost:3000 NODE_OPTIONS=\"--import tsx\" cucumber-js --tags @browser --parallel 2'",
    "coverage:report": "NODE_OPTIONS=\"--import tsx\" tsx scripts/generate-coverage.ts",
    "coverage:clean": "rm -rf test-results/coverage test-results/report && mkdir -p test-results/coverage test-results/report"
  },
